<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".find_home_friends">

    <ImageButton
        android:id="@+id/imageButton31"
        android:layout_width="32dp"
        android:layout_height="43dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="48dp"
        android:background="#00FFFFFF"
        android:clickable="true"
        android:onClick="goBack"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/arrow_left" />

    <ImageButton
        android:id="@+id/imageButton32"
        android:layout_width="32dp"
        android:layout_height="38dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="24dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/list" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="Ev Arkadaşlaşı Bul"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/imageButton32"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toEndOf="@+id/imageButton31"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/unicef_button5"
        android:layout_width="355dp"
        android:layout_height="214dp"
        android:layout_marginTop="44dp"
        android:background="#00FFFFFF"
        android:onClick="openUnicefBurs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView51"
        app:srcCompat="@drawable/scholarship_stick" />

    <ImageButton
        android:id="@+id/imageButton33"
        android:layout_width="349dp"
        android:layout_height="213dp"
        android:layout_marginTop="32dp"
        android:background="#00FFFFFF"
        android:onClick="openUGDDBurs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unicef_button5"
        app:srcCompat="@drawable/scholarship_stick" />

    <ImageView
        android:id="@+id/imageView46"
        android:layout_width="147dp"
        android:layout_height="149dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/unicef_button5"
        app:layout_constraintStart_toStartOf="@+id/unicef_button5"
        app:layout_constraintTop_toTopOf="@+id/unicef_button5"
        app:layout_constraintVertical_bias="0.461"
        app:srcCompat="@drawable/share" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="158dp"
        android:layout_height="69dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/dialog_stick_white"
        android:contextClickable="false"
        android:text="Paylaşımlı Ev Bul"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/unicef_button5"
        app:layout_constraintEnd_toEndOf="@+id/unicef_button5"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView46"
        app:layout_constraintTop_toTopOf="@+id/unicef_button5"
        app:layout_constraintVertical_bias="0.206" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="144dp"
        android:layout_height="69dp"
        android:background="@drawable/dialog_stick_white"
        android:contextClickable="false"
        android:text="Ev Arkadaşı Bul"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton33"
        app:layout_constraintEnd_toEndOf="@+id/imageButton33"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toEndOf="@+id/imageView49"
        app:layout_constraintTop_toTopOf="@+id/imageButton33"
        app:layout_constraintVertical_bias="0.166" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="detaylar için tıklayınız."
        app:layout_constraintBottom_toBottomOf="@+id/unicef_button5"
        app:layout_constraintEnd_toEndOf="@+id/unicef_button5"
        app:layout_constraintStart_toEndOf="@+id/imageView46"
        app:layout_constraintTop_toBottomOf="@+id/textView52" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="detaylar için tıklayınız."
        app:layout_constraintBottom_toBottomOf="@+id/imageButton33"
        app:layout_constraintEnd_toEndOf="@+id/imageButton33"
        app:layout_constraintStart_toEndOf="@+id/imageView49"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />

    <ImageView
        android:id="@+id/imageView49"
        android:layout_width="138dp"
        android:layout_height="141dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/curved"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton33"
        app:layout_constraintStart_toStartOf="@+id/imageButton33"
        app:layout_constraintTop_toTopOf="@+id/imageButton33"
        app:layout_constraintVertical_bias="0.486"
        app:srcCompat="@drawable/photo" />
</androidx.constraintlayout.widget.ConstraintLayout>