        android:text="SABANCI BURSU"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/sabanci_button"
        app:layout_constraintEnd_toEndOf="@+id/sabanci_button"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toEndOf="@+id/imageView24"
        app:layout_constraintTop_toTopOf="@+id/sabanci_button"
