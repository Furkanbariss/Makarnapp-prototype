    <TextView
        android:id="@+id/textView23"
        android:layout_width="152dp"
        android:layout_height="65dp"
        android:text="detaylar için tıklayınız."
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/unicef_button"
        app:layout_constraintEnd_toEndOf="@+id/unicef_button"
