    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="130dp"
        android:layout_height="121dp"
        android:background="@drawable/dialog_stick_white"
        app:layout_constraintBottom_toBottomOf="@+id/sabanci_button"
        app:layout_constraintEnd_toEndOf="@+id/sabanci_button"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="@+id/sabanci_button"
        app:layout_constraintTop_toTopOf="@+id/sabanci_button"
